#include "stdafx.h"
#include <iostream>
#include "conio.h"

using namespace std;

void main()
{
	cout<<"Himno a Bolivia"<<endl;
	cout<<"Bolivianos el hado propicio"<<endl;
	cout<<"corono nuestros votos y anhelos"<<endl;
	cout<<"es ya libre, ya libre este suelo"<<endl;
	cout<<"ya ceso su servil condicion"<<endl;
	getch();
}


***Pseudocodigo***

Proceso HimnoaBolivia
	Escribir "Bolivianos el hado propicio"
	Escribir "corono nuestros votos y anhelos"
	Escribir "es ya libre, ya libre este suelo"
	Escribir "ya ceso su servil condicion"
	
FinProceso
